                "enum": ["Fire", "Earth", "Air", "Water"]
              },
              "modality": {
                "type": "string",
                "description": "Modality of the sun sign",
                "enum": ["Cardinal", "Fixed", "Mutable"]
              },
              "polarity": {
                "type": "string",
                "description": "Polarity of the sun sign",
                "enum": ["Positive", "Negative"]
              },
              "birthDate": {
                "type": "string",
                "format": "date",
                "description": "Birth date in YYYY-MM-DD format"
              },
              "birthTime": {
                "type": "string",
                "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$",
                "description": "Birth time in HH:MM format"
              },
              "birthPlace": {
                "type": "string",
